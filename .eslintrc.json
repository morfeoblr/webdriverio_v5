{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "new-cap": 0,
    "max-len": 0,
    "arrow-body-style": 0,
    "no-param-reassign": 0,
    "no-useless-escape": 0,
    "no-unused-vars": 0,
    "linebreak-style": 0,
    "no-console": [
      "warn",
      {
        "allow": [
          "warn",
          "error",
          "info"
        ]
      }
    ]
  }
}